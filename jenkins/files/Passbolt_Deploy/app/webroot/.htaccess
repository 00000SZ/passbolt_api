AuthUserFile /var/lib/jenkins/jobs/Passbolt_Deploy/workspace/deploy/app/webroot/.htpasswd
AuthType Basic
AuthName "Authentic black bro"
Require valid-user

<IfModule mod_rewrite.c>
    RewriteEngine On
		RewriteBase /deploy
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.*)$ index.php [QSA,L]
</IfModule>

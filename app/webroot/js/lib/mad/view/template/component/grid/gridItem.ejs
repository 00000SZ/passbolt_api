<tr id="<%= id %>" <%= (el) -> el.data(item.constructor.fullName, item) %>>

<% for (var i in columnModels) { %>

	<td class="js_grid_column_<%= columnModels[i].name %> <%= columnModels[i].header.css.join(' ') %>">
		<span title="<%== titles[columnModels[i].name] %>">
			<%== values[columnModels[i].name] %>
		</span>
	</td>

<% } %>

</tr>
